using System;
using System.Collections;



class Demo
{
public static void Main()
{
ArrayList a=new ArrayList(2);
ArrayList a1=new ArrayList(5);
ArrayList a2=new ArrayList(2);

a.Add(1);
a.Add(2);
a.Add(3);
a.Add(4);
a.Add(5);
a.Add(6);

a1.Add(1);
a1.Add(2);
a1.Add(3);

a2.Add(1);

Console.WriteLine("1st list capacity is "+a.Capacity);
Console.WriteLine("2nd list capacity is "+a1.Capacity);
Console.WriteLine("3rd list capacity is "+a2.Capacity);
}
}


