
using System;
using System.Collections;

class UCC {

	
	static public void Main()
	{

		// Creating ArrayList
	ArrayList My_array = new ArrayList();

	                           	// Adding elements in the  My array ArrayList
	                         	// This ArrayList contains elements of different types
		My_array.Add(98);
		My_array.Add("coder");
		My_array.Add(null);
		My_array.Add("Umesh");
		My_array.Add(1234567878);
		My_array.Add("asdf");

		           // Accessing the elements of My_array ArrayList Using foreach loop
	
		foreach(var elements in My_array)
		{
			Console.WriteLine(elements);
		}
	}
}
