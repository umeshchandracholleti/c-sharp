using System;
using System.Collections;



class Demo
{
public static void Main()
{
ArrayList a=new ArrayList(10);

a.Add(1);
a.Add(2);
a.Add(3);
a.Add(4);
a.Add(5);
a.Add(6);

Console.WriteLine("arraylist count is "+a.Count);

a.Remove(3);
Console.WriteLine("count After Remove is "+a.Count);

a.RemoveAt(2);
Console.WriteLine("count after RemoveAt() is "+a.Count);

a.RemoveRange(1,2);
Console.WriteLine("count after RemoveRange is "+a.Count);

a.Clear();
Console.WriteLine("count after clear() is "+a.Count);
}
}
